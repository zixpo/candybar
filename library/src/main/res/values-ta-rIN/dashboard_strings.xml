    <!-- Tap Intro -->
    <string name="tap_intro_home_navigation">Navigation drawer</string>
    <string name="tap_intro_home_navigation_desc">வழிசெலுத்தல் டிராயரைத் திறக்க, இங்கே தட்டவும் அல்லது திரையின் இடது விளிம்பிலிருந்து ஸ்வைப் செய்யவும்</string>
    <string name="tap_intro_home_apply">Apply icon pack</string>
    <string name="tap_intro_home_apply_desc">உங்கள் தற்போதைய துவக்கிக்கு %sஐப் பயன்படுத்த இங்கே தட்டவும். மற்ற துவக்கிகளுக்குப் பயன்படுத்த நீண்ட நேரம் அழுத்தவும்.</string>
    <string name="tap_intro_icons_search">தேடல்</string>
    <string name="tap_intro_icons_search_desc">ஐகானைத் தேட இங்கே தட்டவும்</string>
