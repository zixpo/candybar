    <string name="tap_intro_request_send">Send icon request</string>
    <string name="tap_intro_request_send_desc">Tap here to send selected missed apps</string>
    <string name="tap_intro_request_premium">Premium icon request</string>
    <string name="tap_intro_request_premium_desc">பிரீமியம் ஐகான் கோரிக்கை தொகுப்பை வாங்க இங்கே தட்டவும்,
